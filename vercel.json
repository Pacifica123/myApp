{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    },
    {
      "source": "/web_part/(.*)\\.js",
      "destination": "/web_part/$1.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/web_part/(.*)\\.css",
      "destination": "/web_part/$1.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ]
}