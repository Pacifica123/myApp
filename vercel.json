{
    "version": 2,
    "builds": [
      {
        "src": "angular.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/my-app"
        }
      }
    ],
    "routes": [
      {
        "src": "/web_part/(.*)",
        "dest": "/web_part/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/web_part/$1"
      }
    ]
  }
  